{
  "name": "cypress-ld-filter",
  "version": "1.0.0",
  "description": "Filter cypress tests using LaunchDarkly feature flags",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "cypress": "npm run build && cypress run"
  },
  "files": [
    "/lib/**/**"
  ],
  "keywords": [
    "cypress",
    "cypress-plugin"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/debug": "^4.1.7",
    "@types/find-test-names": "^1.14.0",
    "cypress": "^9.5.0",
    "prettier": "^2.5.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "debug": "^4.3.3",
    "fast-glob": "^3.2.11",
    "find-test-names": "^1.14.1",
    "launchdarkly-node-server-sdk": "^6.3.0"
  }
}
